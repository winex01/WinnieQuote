{
    "name": "winnie/winniequote",
    "description": ":Laravel package that generate random quote.",
    "license": "MIT",
    "authors": [
        {
            "name": "Winnie A. Damayo",
            "email": "winnie131212592@gmail.com",
            "homepage": "https://github.com/winex01/"
        }
    ],
    "homepage": "https://github.com/winnie/winniequote",
    "keywords": ["Laravel", "WinnieQuote", "Random Quote Package", "Programming Quotes Package"],
    "require": {
        "illuminate/support": "^5.5 || ^6.0 || ^7.0 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Winnie\\WinnieQuote\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Winnie\\WinnieQuote\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Winnie\\WinnieQuote\\WinnieQuoteServiceProvider"
            ],
            "aliases": {
                "WinnieQuote": "Winnie\\WinnieQuote\\Facades\\WinnieQuote"
            }
        }
    }
}
